{
  "name": "materialize-strict",
  "description": "This project aims towards a more strict implementation of Googles Material Design, than original materialize-css.",
  "author": "Alvin Wang, Alan Chang, Sebastian Kocks",
  "homepage": "https://www.wesotion.com/materialize",
  "version": "1.0.0-alpha5",
  "main": "dist/materialize-strict.js",
  "style": "dist/materialize-strict.css",
  "sass": "src/sass/materialize.scss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/wesotion/materialize-strict.git"
  },
  "bugs": {
    "url": "https://github.com/wesotion/materialize-strict/issues"
  },
  "scripts": {
    "build": "webpack",
    "build:verbose": "webpack --display-modules",
    "build:watch": "webpack -w",
    "dev": "webpack-dev-server",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "clrscr && nyc ava && nyc report --reporter=html",
    "test:watch": "nodemon --watch 'src/' -e ts,html --exec 'yarn test'",
    "coverage-serve": "node serve-coverage.js"
  },
  "engine": "node >= 6",
  "peerDependencies": {
    "animejs": "^2.2.0",
    "cash-dom": "^1.3.5",
    "node-waves": "^0.7.5",
    "normalize.css": "^7.0.0",
    "roboto-npm-webfont": "^1.0.1"
  },
  "devDependencies": {
    "@types/jsdom": "^11.0.4",
    "@types/node": "^8.0.57",
    "@types/sinon": "^4.1.2",
    "animejs": "^2.2.0",
    "autoprefixer-loader": "^3.2.0",
    "ava": "^0.24.0",
    "browser-env": "^3.2.4",
    "cash-dom": "^1.3.5",
    "clean-webpack-plugin": "^0.1.17",
    "clrscr": "^1.0.4",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "express-static-livereload": "^1.0.1",
    "extract-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.1",
    "jsdom": "^11.5.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.5",
    "nyc": "^11.3.0",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.9.1",
    "webpack-dev-server": "2.7.1"
  },
  "files": [
    "dist",
    "src/**/*.ts",
    "src/**/*.scss",
    "LICENSE"
  ],
  "ava": {
    "files": [
      "./_dist/**/*.spec.js"
    ],
    "source": [
      "./_dist/**/*.js"
    ],
    "require": [
      "./test/globals.js"
    ]
  },
  "nyc": {
    "extensions": [
      ".ts"
    ],
    "include": [
      "_dist/**/*.js"
    ],
    "exclude": [
      "**test**",
      "**/*.spec.js"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {
    "rxjs": "^5.5.5"
  }
}
